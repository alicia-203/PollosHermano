@model IEnumerable<PollosHermano.Models.RegionTipoNotificacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Region)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoNotificacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Region.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoNotificacion.Nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RegionTipoNotificacionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RegionTipoNotificacionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RegionTipoNotificacionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
